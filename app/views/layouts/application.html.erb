<!DOCTYPE html>
<html>
<head>
  <title><%= @title %> | railsadmintest</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render "application/primary_nav" %>

  <div id="main-content">
    <% unless controller_name == "welcome" %><h1><%= @title %></h1><% end %>

    <%= yield %>
  </div>

  <%= render "application/footer" %>

</body>
</html>
